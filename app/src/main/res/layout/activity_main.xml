    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.suitepad.sessionone.MainActivity">

    <Button
        android:id="@+id/button_ali"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ali" />

    <Button
        android:id="@+id/button_mohamed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mohamed" />

    <Button
        android:id="@+id/button_hussein"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hussein" />

    <Button
        android:id="@+id/button_hamda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hamdaa" />

</LinearLayout>
